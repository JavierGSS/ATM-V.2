event.target.value));\n  };\n\n  const handleSubmit = (event) => {\n    let newTotal = isDeposit ? totalState + deposit : totalState - deposit;\n    setTotalState(newTotal);\n    setValidTransaction(false);\n    event.target.reset();\n    event.preventDefault();\n  };\n\n  const handleModeSelect = (event) => {\n    setAtmMode(event.target.value);\n    event.target.value == \"Deposit\" ? setIsDeposit(true) : setIsDeposit(false);\n    return event.target.value;\n\n  }\n\n  return (\n    <form onSubmit={handleSubmit}>\n      <h1 style={{color: 'rgb(35, 35, 87)'}}>App Banco Chévere</h1>\n      <h2 id=\"total\">{status}</h2>\n      <label>Elija el servicio deseado:</label><br/><br/>\n        <select onChange={(e) => handleModeSelect(e)} name=\"mode\" id=\"mode-select\">\n          <option id=\"no-selection\" value=\"\"></option>\n          <option id=\"deposit-selection\" value=\"Deposit\">Depósito</option>\n          <option id=\"cashback-selection\" value=\"Cash Back\">Retiro</option>\n        </select>\n      <ATMDeposit\n        onChange={handleChange}\n        isDeposit={isDeposit}\n        atmMode={atmMode}\n        isValid={validTransaction}>\"\n      </ATMDeposit>\n    </form>\n  );\n};\n\nexport default App;\n","/Users/babalu/Desktop/Desktop - Perikles/MIT/Week 15/ATM-V.2/src/index.js",[],[],"/Users/babalu/Desktop/Desktop - Perikles/MIT/Week 15/ATM-V.2/src/reportWebVitals.js",[],[],"/Users/babalu/Desktop/Desktop - Perikles/MIT/Week 15/ATM-V.2/src/App.jsx",["38","39","40"],[],{"ruleId":"41","severity":1,"message":"42","line":9,"column":11,"nodeType":"43","messageId":"44","endLine":9,"endColumn":13},{"ruleId":"41","severity":1,"message":"42","line":9,"column":35,"nodeType":"43","messageId":"44","endLine":9,"endColumn":37},{"ruleId":"41","severity":1,"message":"42","line":56,"column":24,"nodeType":"43","messageId":"44","endLine":56,"endColumn":26},{"ruleId":"41","severity":1,"message":"42","line":9,"column":11,"nodeType":"43","messageId":"44","endLine":9,"endColumn